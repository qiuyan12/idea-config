<templateSet group="my_notes">
  <template name="mmm" value="/**&#10; * @Title:$Title$ @version V1.0.0&#10; * @author: edwin @date $date$ $time$ &#10; * @description&#10; * @param $param$&#10; * @return $return$&#10; */&#10;" description="方法注解" toReformat="false" toShortenFQNames="true">
    <variable name="Title" expression="methodName()" defaultValue="" alwaysStopAt="true" />
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="time" expression="time()" defaultValue="" alwaysStopAt="true" />
    <variable name="param" expression="methodParameters()" defaultValue="" alwaysStopAt="true" />
    <variable name="return" expression="methodReturnType()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="ccc" value="/**&#10; * @Title:$Title$ @version V1.0.0&#10; * @author: edwin @date $date$ $time$ &#10; * @description&#10; * @return $return$&#10; */" description="类注解" toReformat="false" toShortenFQNames="true">
    <variable name="Title" expression="className()" defaultValue="" alwaysStopAt="true" />
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="time" expression="time()" defaultValue="" alwaysStopAt="true" />
    <variable name="return" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>